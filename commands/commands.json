      "syntax": "a.websitebg (attachment/link)"
    },
    "impersonate": {
      "category": "fun",
      "description": "impersonates someone and makes them say something",
      "syntax": "a.impersonate (mention/user id) (text)"
    },
    "buy": {
      "category": "economy",
      "description": "buys something from the shop",
      "syntax": "a.buy"
    },
    "quote": {
      "category": "fun",
      "description": "quotes someone's message",
      "syntax": "a.quote (while replying to a message)"
    },
    "setstatus": {
      "category": "admin",
      "description": "sets agnabot's status",
      "syntax": "a.setstatus (watching/playing/competing/listening) (text)"
    },
    "balance": {
