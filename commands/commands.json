{
  "commands": {
    "sudo": {
      "category": "git",
      "description": "sudowoodo (executes command in 24/7 command prompt)",
      "syntax": "a.sudo (command)"
    },
    "fetch": {
      "category": "git",
      "description": "fetches git status",
      "syntax": "a.fetch"
    },
    "pull": {
      "category": "git",
      "description": "pulls from github! wow",
      "syntax": "a.pull"
    },
    "reload": {
      "category": "git",
      "description": "reloads the bot",
      "syntax": "a.reload"
    },
    "giveartifact": {
      "category": "admin",
      "description": "gives an artifact of rarity uber",
      "syntax": "a.giveartifact (artifact name)"
    },
    "lock": {
      "category": "admin",
      "description": "locks down or unlocks the bot",
      "syntax": "a.lockdown"
    },
    "mcsudo": {
      "category": "minecraft",
      "description": "makes the minecraft bot say something",
      "syntax": "a.mcsudo (text)"
    },
    "whitelist": {
      "category": "minecraft",
      "description": "makes a request to whitelist you on the minecraft server",
      "syntax": "a.whitelist (minecraft username CASE SENSITIVE)"
    },
    "verify": {
      "category": "minecraft",
      "description": "links your minecraft account to agnabot",
      "syntax": "a.verify"
    },
    "mc": {
      "category": "minecraft",
      "description": "gets the total amount of minecraft characters",
      "syntax": "a.verify"
    },
    "lb": {
      "category": "economy",
      "description": "gives the current leaderboard",
      "syntax": "a.lb"
    },
    "crashlog": {
      "category": "utility",
      "description": "gives the last unhandled crash's log",
      "syntax": "a.crashlog"
    },
    "pet": {
      "category": "pet",
      "description": "with no arguments, this gives you your pet's image\nname renames your pet\ndisown deletes your pet\nimage sets the pet's image\nbackground sets the pet's background\n*color sets the pet's color*\n*subtitle sets the text under the pet's name*",
      "syntax": "a.pet (args) (attach image or add text)"
    },
    "hotel": {
      "category": "hotel",
      "description": "custom channel you can buy in the shop\n\ndelete: deletes the hotel room\nname/rename: renames the channel name\ndescription: sets the channel description\nprivate: makes the channel private\npublic: undoes private\ninvite: invites someone to your hotel room if it is private so they can see it too\nremove: removes an invited person\nlock: makes it so that only you can talk in the hotel room but other people can see it\nunlock: undoes lock",
      "syntax": "a.hotel (args) (add text or mention if needed)"
    },
    "work": {
      "category": "economy",
      "description": "gives you a random amount of agnabucks (50-100) with a 60 second cooldown",
      "syntax": "a.work"
    },
    "setmoney": {
      "category": "admin",
      "description": "sets someone's agnabuck amount",
      "syntax": "a.setmoney (user id/mention) (amount)"
    },
    "inventory": {
      "category": "inventory",
      "description": "views your inventory",
      "syntax": "a.inventory"
    },
    "fish": {
      "category": "economy",
      "description": "fishes! wow\n(requires you to buy a fishing rod in the shop)",
      "syntax": "a.fish"
    },
    "doubleornothing": {
      "category": "economy",
      "description": "has the chance to double your money or delete it",
      "syntax": "a.doubleornothing (amount of money, 1-100)"
    },
    "donate": {
      "category": "economy",
      "description": "donates to someone",
      "syntax": "a.donate (mention) (amount)"
