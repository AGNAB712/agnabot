    },
    "redeem": {
      "category": "economy",
      "description": "gives you agnabucks based on your server level (5 minute cooldown)",
      "syntax": "a.redeem"
    },
    "ask": {
      "category": "fun",
      "description": "asks agnabot something (uses cleverbot ai)",
      "syntax": "a.ask (message)"
    },
    "balbg": {
      "category": "fun",
